{
  "name": "music-mashup",
  "version": "2.0.0",
  "description": "Music Mashup - Your tasteful resource for music knowledge!",
  "author": "Carl Hamilton <info@carlhamilton.co.uk>",
  "private": true,
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "jest",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cahamilton/music-mashup"
  },
  "bugs": {
    "url": "https://github.com/cahamilton/music-mashup/issues"
  },
  "keywords": [
    "music",
    "mashup"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.3",
    "debug": "^2.6.8",
    "dompurify": "^1.0.8",
    "dotenv": "^8.2.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "http-status-codes": "^1.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.3",
    "jsdom": "^13.1.0",
    "lastfm": "^0.9.2",
    "morgan": "^1.8.2",
    "musicbrainz-api": "^0.5.2",
    "normalize.css": "^7.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.4.3",
    "style-loader": "^0.18.1",
    "svgo-loader": "^1.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.3.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "webpack-dev-server": "^3.1.11"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/assets/**/*.{js,jsx}",
      "**/routes/**/*.{js,jsx}",
      "!**/container/**",
      "!**/coverage/**",
      "!**/public/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleNameMapper": {
      "\\.(css|pcss|scss|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "notify": true,
    "testURL": "http:///127.0.0.1"
  }
}
